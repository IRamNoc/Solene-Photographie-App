# Configuration pour l'environnement de production
NODE_ENV=production

# URL de l'API (production)
VITE_API_URL=https://api.votre-domaine.com

# Configuration Supabase (production)
VITE_SUPABASE_URL=https://your-prod-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-prod-anon-key

# Configuration Cloudinary (production)
VITE_CLOUDINARY_UPLOAD_URL=https://api.cloudinary.com/v1_1/your-prod-cloud-name/image/upload
VITE_CLOUDINARY_PRESET=your-prod-upload-preset

# Configuration AWS S3 (optionnel - production)
# VITE_AWS_S3_BUCKET=your-prod-bucket
# VITE_AWS_REGION=your-region
# VITE_AWS_ACCESS_KEY=your-prod-access-key
# VITE_AWS_SECRET_KEY=your-prod-secret-key

# Configuration de sécurité
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Configuration SSL/HTTPS
HTTPS=true

# Domaine de production
VITE_DOMAIN=https://votre-domaine.com
# Configuration npm pour résoudre les problèmes de dépendances optionnelles
# Force npm à installer les dépendances optionnelles appropriées pour la plateforme
optional=true
fund=false
audit=false

# Évite les problèmes de cache avec les dépendances optionnelles
cache-max=0

# Force la réinstallation des dépendances optionnelles
force=false
prefer-offline=false